{
    "name": "Soundbridge",
    "version": "0.2.0",
    "description": "John Morton's Soundbridge",
    "scripts": {
        "lint": "eslint",
        "build": "NODE_ENV='production' webpack --config config/webpack.config.js",
        "dev": "concurrently \"npm:dev:server\" \"npm:dev:frontend\" \"npm:type-check:watch\"",
        "dev:frontend": "NODE_ENV='development' webpack --watch --config config/webpack.config.js",
        "dev:server": "nodemon ./bin/www",
        "type-check": "tsc -p tsconfig.json ./src/**/*.ts",
        "type-check:watch": "npm run type-check -- --watch",
        "test": "mocha --config config/mocharc.json src/**/*.spec.js",
        "apptest": "mocha ./test/app-test.js"
    },
    "author": "Michael Sperone",
    "license": "ISC",
    "dependencies": {
        "bootstrap": "^4.5.2",
        "dotenv": "^7.0.0",
        "express": "^4.17.1",
        "lodash.debounce": "^4.0.8",
        "lodash.throttle": "^4.1.1",
        "preact": "^10.11",
        "preact-router": "^3.2.1",
        "react-audio-graph": "^0.0.3",
        "react-bootstrap": "^1.3.0",
        "socket.io": "^4.5.3",
        "socket.io-client": "^3.1.2",
        "source-map-loader": "^0.2.4",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.16.12",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
        "@babel/plugin-transform-react-jsx": "^7.9.4",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@babel/register": "^7.16.9",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.3",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "concurrently": "^5.2.0",
        "copy-webpack-plugin": "^10.2.1",
        "css-loader": "^3.5.3",
        "eslint": "^7.26.0",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.5.2",
        "mocha": "^9.2.0",
        "nodemon": "^2.0.4",
        "prop-types": "^15.7.2",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "supertest": "^4.0.2",
        "ts-node": "^10.4.0",
        "typescript": "^3.9.3",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.3"
    }
}
