{
    "name": "Soundbridge",
    "version": "0.2.0",
    "description": "John Morton's Soundbridge",
    "scripts": {
        "lint": "eslint",
        "server": "pm2 start ecosystem.config.js",
        "build": "webpack --env.NODE_ENV=production --display-error-details --config config/webpack.config.js",
        "dev": "concurrently \"npm:dev:frontend\" \"npm:test\" \"npm:type-check:watch\"",
        "dev:frontend": "NODE_ENV='development' webpack --watch --display-error-details --config config/webpack.config.js",
        "dev:server": "nodemon ./bin/www",
        "type-check": "tsc -p config/tsconfig.json",
        "type-check:watch": "npm run type-check -- --watch",
        "test": "mocha",
        "apptest": "mocha ./test/app-test.js"
    },
    "author": "Michael Sperone",
    "license": "ISC",
    "dependencies": {
        "@babel/preset-react": "^7.9.4",
        "bootstrap": "^4.5.2",
        "dotenv": "^7.0.0",
        "express": "^4.17.1",
        "lodash.debounce": "^4.0.8",
        "lodash.throttle": "^4.1.1",
        "mini-css-extract-plugin": "^0.8.2",
        "pm2": "^4.4.1",
        "preact": "^10.4.4",
        "preact-router": "^3.2.1",
        "react-audio-graph": "^0.0.3",
        "react-bootstrap": "^1.3.0",
        "socket.io": "^3.1.2",
        "socket.io-client": "^3.1.2",
        "source-map-loader": "^0.2.4",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
        "@babel/plugin-transform-react-jsx": "^7.9.4",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/register": "^7.9.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "concurrently": "^5.2.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.5.3",
        "eslint": "^7.26.0",
        "html-webpack-plugin": "^4.3.0",
        "mocha": "^7.1.2",
        "nodemon": "^2.0.4",
        "prop-types": "^15.7.2",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "supertest": "^4.0.2",
        "typescript": "^3.9.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    }
}
